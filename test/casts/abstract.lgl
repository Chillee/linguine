tag coord a is vec2;
tag coord b is vec2;

`u app<`t : coord vec2, `u : coord vec2>(`t v, `t->`u m) {
    return v in `u;
}

void main() {
    a avec = [1., 0.];
    b bvec = [0., 1.];

    a->b atb = [[0., 1.], [1., 0.]];

    b s1 = app<a, b>(avec, atb);
    a s2 = app<a, a>(avec, [[1., 0.], [0., 1.]]);
}