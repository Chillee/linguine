tag a is vec2;
tag b is vec3;
tag c is b;

a x = [1., 2.]:a;
b y = [2., 1., 2.]:b;
c z = [3., 3., 3.]:c;

a->b a_b = [[1., 1., 1.], [1., 1., 1.]]:a->b;
vec2->vec3 v2_v3 = a_b;
mat2x3 m = a_b;
c->a c_a = [[1., 1.], [1., 1.], [1., 1.]]:c->a;
vec2->vec3 v3_v2 = c_a;

print x * a_b + y;
print x * m;
print x * v2_v3;
print z * c_a + x;
print (z * v3_v2) * v2_v3 + z;
print v2_v3 * v3_v2;
print x * (a_b * v3_v2);