void main(vec3 aPosition, vec3 aNormal, vec3 vPosition, vec3 vNormal, vec4->vec4 uProjection, vec4->vec4 uView, vec4->vec4 uModel, vec2 aTexCoord, vec2 vTexCoord) {
  vec3->vec4 v3_v4 = [
    [1., 0., 0.],
    [0., 1., 0.],
    [0., 0., 1.],
    [0., 0., 0.]
  ];

  vTexCoord = aTexCoord;

  vNormal = aNormal;
  vPosition = aPosition;
  vec4 gl_Position = uProjection * uView * uModel * (v3_v4 * aPosition + [0., 0., 0., 1.]);
}